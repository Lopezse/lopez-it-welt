    "cors": "^2.8.5",
    "critters": "^0.0.23",
    "express": "^4.18.2",
    "framer-motion": "^12.18.1",
    "gray-matter": "^4.0.3",
    "langchain": "^0.3.29",
    "lucide-react": "^0.518.0",
    "mysql2": "^3.14.1",
    "next": "^15.3.4",
    "react": "^18",
    "react-country-flag": "^3.1.0",
