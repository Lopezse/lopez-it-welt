    "agenten:kategorie": "node scripts/agenten-dashboard.js --kategorie",
    "enterprise:setup": "node scripts/enterprise-setup.js",
    "enterprise:validate": "node scripts/enterprise-validate.js",
    "enterprise:quality": "node scripts/enterprise-quality.js",
    "enterprise:security": "node scripts/enterprise-security.js",
    "enterprise:monitoring": "node scripts/enterprise-monitoring.js",
    "migrate:md-to-db": "node scripts/md-to-db-migration.js",
    "compliance:check": "node scripts/db-compliance-system.js",
    "agents:run": "node scripts/agenten-system.js",
